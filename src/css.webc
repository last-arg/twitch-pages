<script webc:type="js">
  let result = "";
  const s = "template";
  for (const c of this.collections.all) {
    if (c.template.fileSlug.parsed.ext === ".css") {
      continue;
    }
    const path = c.template.fileSlug.inputPath
    result += `<${s} webc:nokeep webc:import="${c.template.fileSlug.inputPath}"></${s}>`;
  }
  result;
</script>
