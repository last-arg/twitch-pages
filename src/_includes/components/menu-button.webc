<button class="menu-item"
  aria-expanded="false"
  :x-bind:aria-expanded="`(state === '${name}').toString()`"
  :x-on:click="`toggleSidebar('${name}')`"
>
  <svg aria-hidden="true" focusable="false"><use :href="'/public/assets/icons.svg#' + icon"></use></svg>
  <span class="sr-only" @text="name"></span>
</button>

