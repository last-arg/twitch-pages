<template webc:type="render">
function() {
  const valid_names = ["category", "user"]
  if (!valid_names.includes(this.name)) {
    throw new Error(`'name' attribute must be one of these values: ${valid_names.join(", ")}`)
  }
  
  const width = this.global.config.image[this.name].width;
  const height = this.global.config.image[this.name].height;

  return `
  <h2 aria-hidden="true">
    <img width="${width}" height="${height}">
    <p></p>
  </h2>
  `;
}
</template>
