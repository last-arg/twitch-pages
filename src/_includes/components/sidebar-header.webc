<div class="sidebar-header cluster" style="--cluster-justify-content: space-between; --cluster-align-items: stretch;">
  <h2>
    <svg>
      <use :href="'/public/assets/icons.svg#' + icon"></use>
    </svg>
    <span webc:nokeep @html="sb_title"></span>
  </h2>
  <button type="button" class="btn-close" x-on:click="closeSidebar()">
    <span class="sr-only">Close sidebar</span>
    <svg><use href="/public/assets/icons.svg#close"></use></svg>
  </button>
</div>
